[
    {
        "type":"readers.las",
        "filename":"{input_file}"
    },
    {
        "type":"filters.range",
        "limits":"Classification[2:2]"
    },
    {
        "type":"filters.sample",
        "radius":0.5,
        "tag":"GROUND"
    },
    {
        "type":"readers.las",
        "filename":"{input_file}"
    },
    {
        "type":"filters.range",
        "limits":"Classification[6:6]"
    },
    {
        "type":"filters.sample",
        "radius":0.5
    },
    {
        "type":"filters.outlier",
