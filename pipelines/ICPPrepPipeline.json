    },
    {
        "type":"filters.range",
        "limits":"Classification![7:7]",
        "tag":"BUILDINGS"
    },
    {
        "type":"filters.merge",
        "inputs":[
            "GROUND",
            "BUILDINGS"
        ]
    },
    {
        "type":"writers.las",
        "filename":"{output_file}"
    }
